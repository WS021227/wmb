<%if(wglobals.lang == 'cn'){%>
<script>
    var dty_ex = new Date('<%= expire %>'), now_date = new Date(), $dty_tips = $('.dty-tips'), $dty_obj
    let _time_flag = (dty_ex.getTime() - now_date.getTime()) / 1000
    function dty_timeout(time_flag) {
        if (time_flag > 0) {
            var second = Math.floor(time_flag % 60); // 计算秒
            var minite = Math.floor((time_flag / 60) % 60); //计算分
            var hour = Math.floor((time_flag / 3600) % 24); //计算小时
            var day = Math.floor((time_flag / 3600) / 24); //计算天
            $dty_tips.html(`延长惊喜券有效期剩余：${day}天 / ${hour}时 / ${minite}分 / ${second}秒`)
        } else {
            $dty_tips.html(`延长券惊喜已过期`)
            if($dty_obj) window.clearInterval($dty_obj)
        }
    }
    $dty_obj = setInterval(function () {
        dty_timeout(_time_flag)
        _time_flag --;
    }, 1000)

    function receive_ty(){
        $.wSetCookie('_DTY', '1', 86400)
    }
</script>
<%}%>

