        <div id="aps"></div>
    </div>
    <form id="wx_pay_add" name="wx_pay_add" method="post">
        <input type="hidden" name="" value="">
    </form>
    <%- include('../global/foot_script.ejs') %>
    <script type="application/javascript">var pay_dty = '<%=results.dty%>';</script>
    <script src="<%= STATIC %>javascripts/payment/pay_detail.js?v=<%=VERSION%>"></script>
    <script src="https://static.52wmb.com/wmb_js/jquery.qrcode.min.js" type="text/javascript"></script>
</body>
</html>