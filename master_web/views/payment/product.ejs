            <div class="pay-share member-order-list">
                <h2><%=translate('payment_price_list')%></h2>
                <ul></ul>
            </div>
            <div class="pay-sumbit">
                <span><%=translate('payment_price_actual')%>:<b>￥<font></font></b></span>
                <a href="javascript:void(0);"  onclick="submit_payment()"><%=translate('payment_pay_now')%></a>
                <br class="clear"/>
