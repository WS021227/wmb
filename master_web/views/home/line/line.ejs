<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%=translate('bline_overview')%>-邦Line</title>
    <%- include('./head.ejs') %>
</head>

<body>
<%- include('../../global/head.ejs') %>
<% var line_user = results.line_user %>
<%- include('../../home/admin_tab.ejs') %>
<div class="wrapper">
    <div class="line-admin">        
        <!-- 主体内容 -->
        <div class="line-admin-content">
            <%- include('./menu.ejs') %>
            <!-- 右侧内容 -->
            <div class="line-admin-wrapper">
                <div class="container">
                    <!-- 个人介绍 -->
                    <div class="line-admin-module line-person" style="background:url('<%=LINE_IMAGE_PATH%><%=line_user.cover%>') no-repeat;background-size:cover;">
                        <div class="line-person-mask"></div>
                        <div class="content">
                            <a class="edit-btn" href="/user/line/edit"><%=translate('bline_overview_edit')%></a>
                            <div class="position">
                                <div class="head">
                                    <img src="<%=LINE_IMAGE_PATH%><%=line_user.avatar%>"/>
<!--                                    <img src="https://static.52wmb.com/images/userphoto/190_130975786242036984.jpg"/>-->
                                    <div class="person-tag"><%=line_user.company_type_name%></div>
                                </div>
                                <div class="name"><%=line_user.name%><span>(<%=line_user.position%>)</span></div>
                                <div class="sub job">
                                    <%=translate('bline_overview_company')%>：<%=line_user.company_name%>
                                </div>
                                <div class="sub supplier">
                                    <%=line_user.verb%>：<%=line_user.products.slice(0, 50)%>...
                                </div>
                                <div class="ip">
                                    <%=translate('bline_overview_attribution')%>：<%=POS_SHOW(line_user)%>
                                </div>
                                <a href="<%=LINE_ROUTE()%>/line/<%=line_user.user_id%>" target="_blank" class="preview-btn"><%=translate('bline_overview_preview_homepage')%></a>
                            </div>
                        </div>
                    </div>
                    <!-- 数据展示 -->
                    <div class="line-admin-module line-analyse">
                        <div class="content">
                            <div class="analyse-module">
                                <div class="topic">
                                    <span class="title"><%=translate('bline_overview_preview_homepage_data')%></span>
                                    <!--a class="link" href="javasscript:void(0);" target="_blank"><%=translate('bline_overview_promotional_data')%></a-->
                                </div>
