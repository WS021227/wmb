                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- 小组推荐 -->
                    <div class="line-admin-module line-group">
                        <div class="topic">
                           <div class="line-module-tab">
                                <div class="tab-item active"><%=translate('bline_overview_circles_recomm')%></div>
                            </div>
                        </div>
                        <div class="content">
                            <div class="swiper-container line-group-list" id="line_group_list"></div>
                        </div>
                    </div>

                    <!-- 推荐供应商 -->
                    <div class="line-admin-module line-supplier">
                        <div class="topic">
                            <div class="line-module-tab">
                                <div class="tab-item tab active" id="line_recommend_user_tab" data-target="#users_list"><%=translate('bline_overview_people_recomm')%></div>
                                <div class="tab-item tab follow-tab"  data-target="#users_list"><%=translate('bline_overview_my_concerns')%></div>
                                <a href="<%=LINE_ROUTE()%>/members" target="_blank"><%=translate('bline_overview_friends_more')%></a>
                            </div>
                        </div>
                        <div class="content" id="users_list">
                            <div class="line-supplier-list tab-pane active" id="line_recommend_user_list"></div>
                            <div class="line-supplier-list tab-pane" id="line_follow_user_list"></div>
                        </div>
                    </div>
