                                                <div class="type"><%=translate('bline_overview_homepage_visitors')%></div>
                                                <div class="count stats-day" data-all="1" data-st="view"></div>
                                                <div class="day stats-day" data-st="today_view"></div>
                                                <i class="dian un display-none" data-st="uncheck_view">红点</i>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div class="analyse-module">
                                <div class="topic">
                                    <span class="title"><%=translate('bline_overview_people_data')%></span>
                                </div>
                                <div class="content">
                                    <div class="analyse-count">
                                        <a href="/user/line/related">
                                            <div class="count-item">
