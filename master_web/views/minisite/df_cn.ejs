        function sweepstakes_countdown(index, etime, show) {
            console.log(index, etime, show)
            if (!etime) return $('#sweepstakes_list .sweepstakes-' + index + ' .cj-flag').html(show)
            let total = (etime - new Date().getTime()) / 1000
            timeout_sc(index, total)
        }

        function timeout_sc(index, total) {
            if (total <= 0) return $('#sweepstakes_list .sweepstakes-' + index + ' .cj-flag').html('抽奖中')
            show_timeout(index, total)
            setTimeout(function () {
                total--
                timeout_sc(index, total)
            }, 1000)
        }


        function show_timeout(index, time_flag) {
            let t_second = Math.floor(time_flag % 60); // 计算秒
            let t_minite = Math.floor((time_flag / 60) % 60); //计算分
            let t_hour = Math.floor((time_flag / 3600) % 24); //计算小时
            let t_day = Math.floor((time_flag / 3600) / 24); //计算天
            // console.log("距离2022-12-31 23:59:59还有" + t_day + "天" + t_hour + "时" + t_minite + "分" + t_second + "秒")


            swiper2_day = t_day < 10 ? '0' + t_day : t_day
            swiper2_hour = t_hour < 10 ? '0' + t_hour : t_hour
            swiper2_minite = t_minite < 10 ? '0' + t_minite : t_minite
            swiper2_second = t_second < 10 ? '0' + t_second : t_second
            $('#sweepstakes_list .sweepstakes-' + index + ' .cj-flag').html(`<span>${swiper2_day}天</span><span>${swiper2_hour}时</span><span>${swiper2_minite}分</span><span>${swiper2_second}秒</span>`)
            // 最后一小时红色显示
            if (time_flag <= 3600) {
                // $("#second").css("color", "red")
                // $("#minite").css("color", "red")
            }
        }

        // 获取优惠券数据
        function get_coupon_data() {
            let $new_user_coupon = $('#new_user_coupon')
            if($new_user_coupon.length <= 0) return;
            $.ajax('/async/payment/df/coupon', {
                datatype: 'text/json',
                type: 'get',
                success: function (res) {
                    if (res.state == 0) {
                        $new_user_coupon.removeClass('display-none')
                        $.each(res.data.list, function (a, b){
                            let fx = b.vip_level == 'v'?`v-${b.vip_duration}`:b.vip_level,
                                no_buyer_old = parseInt(wg.user.no_buyer_old || 0)
                            if(b.vip_duration == 1 && b.vip_level== 'v' && (no_buyer_old==1 || no_buyer_old==4)) return true
                            let $rec = $('.coupon-receive.coupon-' + fx)
                            $rec.parents('.vs-box').removeClass('display-none')
                            if(b.activity_state != 1){
                                $rec.html('活动已过期')
                            }else if(b.id){
                                if(b.coupon_state != 1){
                                    $rec.html('优惠券无效')
                                }else {
                                    $rec.html('使用优惠券 →').attr('href', `/Alipay?v=${b.vip_level}&m=${b.vip_duration}`)
                                        .attr('target', '_blank')

                                    hav_receive($rec)
                                }
                            }else {
                                $rec.html('领取优惠券').attr('onclick', `get_coupon(this)`)
                                    .data('sid', b.sid).data('v', b.vip_level).data('m', b.vip_duration)
                            }
                        })
                    }
                }
            })
        }

        // 领取优惠券
        function get_coupon(elem) {
            let $elem = $(elem), params = $elem.data()
            $.ajax('/async/payment/df/coupon', {
                data: { setting_id: params.sid },
                type: 'post',
                datatype: 'text/json',
                success: function (res) {
                    if (res.state == 0) {
                        $elem.attr('href', `Alipay?v=${params.v}&m=${params.m}`).attr('target', '_blank').html('使用优惠券 →')
                        hav_receive($elem)
                    }
                }
            })
        }
        function hav_receive($elem){
            $elem.siblings('.lin-img').removeClass('display-none')
        }


