        // 获取抽奖数据
        function get_cj_data() {
            $.ajax('/async/payment/df/sweepstakes', {
                datatype: 'text/json',
                type: 'get',
                success: function (res) {
                    if (res.state == 0) {
                        //奖池总人数
                        cj_sum = res.data.total_user
                        // 抽奖次数
                        cj_num = res.data.opp_count
                        cj_data = res.data.list
                        let is_win = 0, win_prize, win_no,have_count = 0, all_over = false, title_box = ''

                        cj_data.forEach((item, index) => {
                            let text = ""

                            giveaways.giveaways_addres=item.giveaways_addres
                            giveaways.giveaways_prize=item.giveaways_prize

                            let time = new Date().getTime()
                            let endtime = new Date(item.end_time).getTime()
                            let starttime = new Date(item.start_time).getTime()
                            if (item.is_win > 0 && !is_win) {
                                is_win = item.is_win
                                win_prize = item.prize
                                win_no = item.name
                                text = '恭喜您中奖啦！'
                                sweepstakes_countdown(index, null, '已中奖')
                            } else if (!item.have || is_win) {
                                text = "不在该奖池"
                                sweepstakes_countdown(index, null, text)
                            } else {

                                text = "已在该奖池"
                                let _end_time, cj_state = ''
                                if (starttime < time && time < endtime) {
                                    // 抽奖中   不用倒计时
                                    cj_state = '抽奖中'
                                    have_count ++
                                } else if (time < starttime) {
                                    // 未开始
                                    _end_time = starttime
                                    have_count ++
                                } else if (index < 2) {
                                    // 当前批次抽奖结束
                                    cj_state = '抽奖结束'
                                    text = "未抽中"

                                } else {
                                    all_over = true
                                    cj_state = '抽奖结束'

                                }
                                sweepstakes_countdown(index, _end_time, cj_state)
                            }

                            $('#sweepstakes_list .sweepstakes-' + index + ' .sbox').html(`<p>中奖率</p>
                            <p>${Math.ceil((item.win_count / cj_sum) * 100)}%</p>
                            <p>奖池${cj_sum}人，抽${item.win_count}人</p>
                            <p>${text}</p>`)

                            if (time > endtime) {
                                $('#sweepstakes_list .sweepstakes-' + index + ' .sbox').append('<img src="<%= STATIC %>images/sjj_images/cj_over.png">')
                            }
                        })
                        if (is_win > 0) {
                            //
                            $('.toast').html(`<img src="<%= STATIC %>images/sjj_images/liwu.png">
                                    <h3>恭喜,您中奖啦!!!</h3>
                                    <p>您在数据节${win_no}抽奖活动中，抽中了${win_prize}。<br/>请联系客服16621075894，完善收货人信息，七日内发货。</p>`)
                        }else if(have_count > 0){
                            $('.toast').html(`
                            <p style="font-size:22.4px;font-weight:bold;">您还有<span>${have_count}</span>波抽奖机会!</p>
                            <p>即刻微信扫码预约直播抽奖，Iphone14(128G星光白)等您来拿。祝您好运！！！</p>
                            <img src="<%= STATIC %>images/sjj_images/choujiang_ma.png" class="yuyue-ma">`)
                        }else if(all_over){
                            $('.toast').html(`
                            <img src="<%= STATIC %>images/sjj_images/no_gift.png" alt="">
                            <h3>抱歉，这次活动你没有抽中！</h3>
                            <p>感谢您对外贸邦的支持与厚爱，来年数据节我们再战。</p>`)

                        }else{
                            $('.toast').html(`
                                <img src="<%= STATIC %>images/sjj_images/tongzhi.png">
                                <h3>您不在奖池，钻石会员可参加抽奖！</h3>
                                <p>活动期间购买钻石会员,最多可参加三次抽奖;高中奖率,Iphone14等你拿</p>
                                <a href="/Alipay?v=yd">购买钻石会员，参与抽奖 →</a>`)
                        }

                        // 已经填写过
                        if (giveaways.giveaways_addres !== null) {
                            $(".spoil-content .spoil-content-head").remove()
                            let $box2 = $(`
                                    <div class="spoil-content-head">
                                        <p>您已挑选了百元礼品...</p>
                                        <p>您已填写了快递及礼品的信息，确认后的信息不可修改。</p>
                                        <a href="javascript:void(0)" onclick="visit_sun_gift()">查看礼品及快递信息 →</a>
                                    </div>`)
